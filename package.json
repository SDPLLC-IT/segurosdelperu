{
  "name": "seguros-del-peru",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postbuild": "echo '<!DOCTYPE html><html><head><meta http-equiv=\"refresh\" content=\"0; url=/\" /><script>window.location.href = \"/\";</script></head><body></body></html>' > dist/404.html",
    "predeploy": "npm run build && npm run postbuild && echo 'www.segurosdelperu.com' > dist/CNAME",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "javascript-obfuscator": "^4.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "sass": "^1.77.6",
    "vite": "^5.3.1"
  }
}
